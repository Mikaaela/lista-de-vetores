#include <stdio.h>

int main() {
    int vetor[15];

 
    printf("Digite os 15 n√∫meros inteiros:\n");
    for (int i = 0; i < 15; i++) {
        scanf("%d", &vetor[i]);
    }

 
    printf("Vetor armazenado: ");
    for (int i = 0; i < 15; i++) {
        printf("%d ", vetor[i]);
    }

    return 0;
}